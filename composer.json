{
  "name": "contributte/seznamcaptcha",
  "description": "SeznamCaptcha to Nette Framework",
  "keywords": [
    "security",
    "captcha",
    "seznam",
    "nette",
    "forms",
    "contributte"
  ],
  "type": "library",
  "license": [
    "MIT"
  ],
  "homepage": "https://github.com/contributte/seznamcaptcha",
  "authors": [
    {
      "name": "Milan Felix Sulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "nette/di": "^3.0.6",
    "nette/forms": "^3.0.7"
  },
  "require-dev": {
    "ninjify/qa": "^0.12",
    "ninjify/nunjuck": "^0.4",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0"
  },
  "autoload": {
    "classmap": [
      "3rd/"
    ],
    "psr-4": {
      "Contributte\\SeznamCaptcha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5.x-dev"
    }
  }
}
